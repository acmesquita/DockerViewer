 <div class="body"></div>
		<div class="grad"></div>
		<div class="header">
			<div>DockerViewer</div>
      <img src="<%= image_path('logo.png') %>" height="128" width="128" style="padding: 5px"/>
		</div>
  <div class="login">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="field">
        <%= f.email_field :email, autofocus: true, autocomplete: "email" , placeholder: "E-mail" %>
      </div>

      <div class="field">
        <%= f.password_field :password, autocomplete: "off" , placeholder: "Password"%>
      </div>

     
      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
    <% end %>

  </div>
